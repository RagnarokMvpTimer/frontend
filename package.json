    "styled-components": "^5.3.5"
  },
  "devDependencies": {
    "@types/body-scroll-lock": "^3.1.0",
    "@types/node": "^18.19.6",
    "@types/react": "^18.0.5",
    "@types/react-dom": "^18.0.1",
    "@types/styled-components": "^5.1.25",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.23.0",
